<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsh.resource.mapper.MaterialMapper">

    <resultMap id="BaseResultMap" type="com.zsh.resource.domain.Material">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="dosage" column="dosage" jdbcType="VARCHAR"/>
            <result property="minorName" column="minor_name" jdbcType="VARCHAR"/>
            <result property="minorDosage" column="minor_dosage" jdbcType="VARCHAR"/>
            <result property="codimentName" column="codiment_name" jdbcType="VARCHAR"/>
            <result property="codimentDosage" column="codiment_dosage" jdbcType="VARCHAR"/>
            <result property="isDeleted" column="is_deleted" jdbcType="INTEGER"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,dosage,
        minor_name,minor_dosage,codiment_name,
        codiment_dosage,is_deleted,create_time,
        update_time
    </sql>
</mapper>
