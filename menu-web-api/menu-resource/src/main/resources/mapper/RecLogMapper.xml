<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsh.resource.mapper.RecLogMapper">

    <resultMap id="BaseResultMap" type="com.zsh.resource.domain.RecLog">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="dishId" column="dish_id" jdbcType="BIGINT"/>
        <result property="memberId" column="member_id" jdbcType="BIGINT"/>
        <result property="clickNum" column="click_num" jdbcType="INTEGER"/>
        <result property="isCollect" column="is_collect" jdbcType="INTEGER"/>
        <result property="isUp" column="is_up" jdbcType="INTEGER"/>
        <result property="commentNum" column="comment_num" jdbcType="INTEGER"/>
        <result property="isDeleted" column="is_deleted" jdbcType="INTEGER"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,dish_id,member_id,
        click_num,is_collect,is_up,
        comment_num,is_deleted,create_time,
        update_time
    </sql>
    <select id="getAllLogData" resultType="com.zsh.resource.domain.RecLog">
        SELECT id,
               dish_id,
               member_id,
               click_num,
               is_collect,
               is_up,
               comment_num,
               create_time
        FROM rec_log
    </select>
</mapper>
