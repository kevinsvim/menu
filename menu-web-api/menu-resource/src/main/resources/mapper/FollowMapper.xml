<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsh.resource.mapper.FollowMapper">

    <resultMap id="BaseResultMap" type="com.zsh.resource.domain.Follow">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="fansId" column="fans_id" jdbcType="BIGINT"/>
            <result property="memberId" column="member_id" jdbcType="BIGINT"/>
            <result property="isDeleted" column="is_deleted" jdbcType="INTEGER"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,fans_id,member_id,
        is_deleted,create_time,update_time
    </sql>
    <select id="getAllMember" resultType="java.lang.String">
        SELECT id
        FROM member
    </select>
</mapper>
